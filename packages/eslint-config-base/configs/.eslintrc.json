{
  "overrides": [
    {
      "files": [
        "**/*.test.js",
        "**/*.test.jsx"
      ],
      "parser": "babel-eslint",
      "plugins": [
        "prettier"
      ],
      "extends": [
        "@sanv/eslint-config-base",
        "@sanv/eslint-config-tests",
        "@sanv/eslint-config-unicorn",
        "@sanv/eslint-config-import",
        "plugin:prettier/recommended"
      ]
    },
    {
      "files": [
        "**/*.story.js",
        "**/*.story.jsx",
        "**/*.stories.js",
        "**/*.stories.jsx"
      ],
      "parser": "babel-eslint",
      "plugins": [
        "prettier"
      ],
      "extends": [
        "@sanv/eslint-config-base",
        "@sanv/eslint-config-stories",
        "@sanv/eslint-config-unicorn",
        "@sanv/eslint-config-import",
        "plugin:prettier/recommended"
      ]
    },
    {
      "files": [
        "./src/**/*.js",
        "./src/**/*.jsx"
      ],
      "parser": "babel-eslint",
      "plugins": [
        "prettier"
      ],
      "extends": [
        "@sanv/eslint-config-base",
        "@sanv/eslint-config-unicorn",
        "@sanv/eslint-config-import",
        "plugin:prettier/recommended"
      ]
    }
  ]
}
