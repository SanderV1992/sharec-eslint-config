{
  "overrides": [
    {
      "files": [
        "**/*.test.ts",
        "**/*.test.tsx"
      ],
      "parser": "@typescript-eslint/parser",
      "plugins": [
        "prettier",
        "import",
        "unicorn",
        "@typescript-eslint"
      ],
      "parserOptions": {
        "project": "./tsconfig.json",
        "sourceType": "module"
      },
      "extends": [
        "@sanv/eslint-config-base",
        "@sanv/eslint-config-typescript",
        "@sanv/eslint-config-tests",
        "@sanv/eslint-config-tests-typescript",
        "@sanv/eslint-config-unicorn",
        "@sanv/eslint-config-unicorn-typescript",
        "@sanv/eslint-config-import",
        "@sanv/eslint-config-import-typescript",
        "plugin:prettier/recommended",
        "prettier/@typescript-eslint"
      ]
    },
    {
      "files": [
        "**/*.story.ts",
        "**/*.story.tsx",
        "**/*.stories.ts",
        "**/*.stories.tsx"
      ],
      "parser": "@typescript-eslint/parser",
      "plugins": [
        "prettier",
        "import",
        "unicorn",
        "@typescript-eslint"
      ],
      "parserOptions": {
        "project": "./tsconfig.json",
        "sourceType": "module"
      },
      "extends": [
        "@sanv/eslint-config-base",
        "@sanv/eslint-config-typescript",
        "@sanv/eslint-config-stories",
        "@sanv/eslint-config-stories-typescript",
        "@sanv/eslint-config-unicorn",
        "@sanv/eslint-config-unicorn-typescript",
        "@sanv/eslint-config-import",
        "@sanv/eslint-config-import-typescript",
        "plugin:prettier/recommended",
        "prettier/@typescript-eslint"
      ]
    },
    {
      "files": [
        "./src/**/*.ts",
        "./src/**/*.tsx"
      ],
      "parser": "@typescript-eslint/parser",
      "plugins": [
        "prettier",
        "import",
        "unicorn",
        "@typescript-eslint"
      ],
      "parserOptions": {
        "project": "./tsconfig.json",
        "sourceType": "module"
      },
      "extends": [
        "@sanv/eslint-config-base",
        "@sanv/eslint-config-typescript",
        "@sanv/eslint-config-unicorn",
        "@sanv/eslint-config-unicorn-typescript",
        "@sanv/eslint-config-import",
        "@sanv/eslint-config-import-typescript",
        "plugin:prettier/recommended",
        "prettier/@typescript-eslint"
      ]
    }
  ]
}
